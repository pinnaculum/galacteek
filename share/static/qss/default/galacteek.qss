QScrollBar:vertical {
    border: 1px solid grey;
    width: 8px;
    margin: 30px 0px 30px 0;
}

QScrollBar:horizontal {
    border: 1px solid grey;
    height: 8px;
    margin: 0px 30px 0 30px;
}

QScrollBar::handle:horizontal {
    background-color: #4b9fa2;
    min-width: 30px;
}

QScrollBar::handle:vertical {
    background-color: #4b9fa2;
